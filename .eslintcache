s) {\n    super(props);\n\n    this.deleteCourse = this.deleteCourse.bind(this);\n\n    this.state = { courses: [] };\n  }\n\n  componentDidMount() {\n    axios\n      .get(\"http://localhost:5000/coursereviews/\")\n      .then((response) => {\n        this.setState({ courses: response.data });\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  }\n\n  deleteCourse(id) {\n    axios\n      .delete(`http://localhost:5000/coursereviews/${id}`)\n      .then((response) => {\n        console.log(response.data);\n      });\n\n    this.setState({\n      courses: this.state.courses.filter((el) => el._id !== id), // update courses array\n    });\n  }\n\n  courseList() {\n    return this.state.courses.map((currentcourse) => {\n      return (\n        <Course\n          course={currentcourse}\n          deleteCourse={this.deleteCourse}\n          key={currentcourse._id} // must-have\n        />\n      );\n    });\n  }\n\n  render() {\n    return (\n      <div>\n        <h3>Reviewed Courses</h3>\n        <table className=\"table\">\n          <thead className=\"thead-light\">\n            <tr>\n              <th>University</th>\n              <th>Subject</th>\n              <th>Code</th>\n              <th>Name</th>\n              <th>Semester</th>\n              <th>Professor</th>\n              <th>Rating</th>\n              <th>Author</th>\n              <th>Actions</th>\n            </tr>\n          </thead>\n          <tbody>{this.courseList()}</tbody>\n        </table>\n      </div>\n    );\n  }\n}\n","/Users/zhixiangteoh/Desktop/course-review-mern/frontend/src/components/EditCourseReview.js",[],"/Users/zhixiangteoh/Desktop/course-review-mern/frontend/src/components/Course.js",[],["44","45"],"/Users/zhixiangteoh/Desktop/course-review-mern/frontend/src/components/Login.js",[],"/Users/zhixiangteoh/Desktop/course-review-mern/frontend/src/components/Register.js",[],{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","s